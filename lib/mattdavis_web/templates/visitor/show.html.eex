<h2>Show Visitor</h2>

<ul>

  <li>
    <strong>Email:</strong>
    <%= @visitor.email %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @visitor.name %>
  </li>

  <li>
    <strong>What:</strong>
    <%= @visitor.what %>
  </li>

</ul>

<span><%= link "Edit", to: visitor_path(@conn, :edit, @visitor) %></span>
<span><%= link "Back", to: visitor_path(@conn, :index) %></span>
